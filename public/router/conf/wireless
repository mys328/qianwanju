config wifi-device  radio0
	option type     mac80211
	option channel  13
	option hwmode	11g
	option path	'platform/ar934x_wmac'
	option htmode	HT40
	option noscan 1
	option country CN

config wifi-iface
	option device   radio0
	option network  lan
	option mode     ap
	option ssid     2GAP
	option encryption psk2
	option key  purplediamond934

config wifi-device  radio1
	option type     mac80211
	option channel  157
	option hwmode	11a
	option path	'pci0000:00/0000:00:00.0'
	option htmode	HT40
	option noscan 1
	option country CN

config wifi-iface
	option device   radio1
	option network  lan
	option mode     ap
	option ssid     5GAP
	option encryption psk2
	option key  purplediamond934

